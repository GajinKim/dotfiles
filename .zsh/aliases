# General aliases
alias desktop='cd ~/Desktop'
alias Desktop='cd ~/Desktop'
alias downloads='cd ~/Downloads'
alias Downloads='cd ~/Downloads'
alias Workspace='cd ~/Workspace'
alias workspace='cd ~/Workspace'
alias ws='cd ~/Workspace'
alias refresh='source ~/.zshrc' # I'm lazy


# Git aliases
alias g="git"
alias ga="git add"
alias gco="git checkout"
alias push="git push"
alias pull="git pull"
alias commit="git commit -m"
alias uncommit="git reset HEAD~1"

# Workflow aliases
alias n='nvim' # what can I say, I'm lazy
alias venv='source ./venv/bin/activate' # Save a lil bit of time
alias :q="exit"

# ssh aliases
alias keys='cd ~/Documents/.keys'

